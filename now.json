{
  "version": 2,
  "name": "login-service",
  "alias": "loginservice.danieltrevino.se",
  "regions": ["arn1"],
  "builds": [{ "src": "src/index.ts", "use": "@now/node@canary" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@mongo-database-url"
  }
}
